<div id="<%= dom_id city %>">

  <p>
    <strong >Nombre:</strong>
    <%= city.nombre %>
  </p>
  
<table class="table table-success table-striped table-bordered">
  <thead >
      <th scope="col">Clientes</th>
    </tr>
  </thead>
  <tbody style="color: green">
    <% @clients.each do |client| %>
      <tr>
      <td> <%= client.nombre %> </td>
      </tr>
   <% end %>
   </tbody>
</table>
</div>
